{
    "name": "dusanbre/laravel-tolgee",
    "version": "1.0.0",
    "description": "This is my package laravel-tolgee",
    "keywords": [
        "Dusan Antonijevic",
        "laravel",
        "laravel-tolgee"
    ],
    "homepage": "https://github.com/dusanbre/laravel-tolgee",
    "license": "MIT",
    "authors": [
        {
            "name": "Dusan Antonijevic",
            "email": "dusan.antonijevic2404@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelTolgee\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelTolgee\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-tolgee --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTolgee\\LaravelTolgeeServiceProvider"
            ],
            "aliases": {
                "LaravelTolgee": "LaravelTolgee\\Facades\\LaravelTolgee"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
